{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],

    "noImplicitAny":      true,
    "noImplicitReturns":  true,
    "noUnusedLocals":     true,
    "noUnusedParameters": true,
    "strictNullChecks":   false,

    "removeComments":     true,
    "inlineSourceMap":    true,
    "preserveConstEnums": true
  }
}